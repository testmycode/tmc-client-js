{
  "name": "tmc-client-js",
  "version": "0.0.9",
  "description": "Client for TMC",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "MIT OR Apache-2.0",
  "exports": {
    ".": {
      "*": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      }
    }
  },
  "scripts": {
    "lint": "eslint src",
    "prepublish": "mkdir -p build && babel src/ -d build/ && npm run types",
    "types": "tsc -p jsconfig.json",
    "build": "NODE_ENV=production webpack --mode=production --env prod",
    "dev": "webpack --progress --watch --mode=development --env dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testmycode/tmc-client-js.git"
  },
  "keywords": [
    "tmc"
  ],
  "author": "",
  "bugs": {
    "url": "https://github.com/testmycode/tmc-client-js/issues"
  },
  "homepage": "https://github.com/testmycode/tmc-client-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "core-js": "^3.26.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.2.0",
    "npm": "^9.1.2",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "yargs": "^17.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "encoding": "^0.1.13",
    "fetch-polyfill": "^0.8.2",
    "store": "^2.0.12",
    "unfetch": "^4.2.0"
  },
  "browserslist": "> 0.25%, not dead"
